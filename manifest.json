{
  "name": "Capture and Compare Full Screen",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Screen capture your current page and compare to gold copies!",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
      "tabs",
      "http://*/*",
      "https://*/*",
      "<all_urls>",
      "notifications"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
